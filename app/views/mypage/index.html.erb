                <p><%= table_data[:summary_num] %></p>
                <% unless table_data[:summary_num] > 1 %>
                <span>
                  <p>summary</p>
                </span>
                <% else %>
                <span>
                  <p>summaries</p>
                </span>
                <% end %>
                <% end %>
              </th>
            </tr>
            <% end %>
          </form>
          <% end %>
