      <% if @is_login_user || @user.public_flg %>
      <div class="article-table">
        <% if @current_tab == "main" %>
        <table class="summary-table active main">
        <% else %>
        <table class="summary-table main">
        <% end %>
          <tr class="table-top">
            <th id="article-title-top">
              <div class="article-checkbox">
                <%= check_box 'check_all', 0, 
                {:id => "main-check-all", :onclick => "checkAll('main-check-all', 'main-check-form')"}, 0, false %>
              </div>
              TITLE
            </th>
            <th id="summary-number-top">summary</th>
          </tr>
          <% if @main_articles_table %>
          <form id="main-check-form" name="main_checkbox">
            <% @main_articles_table.each_with_index do |table_data, i| %>
            <tr>
              <th>
                <div>
                  <div class="article-checkbox">
                    <% if @is_login_user %>
                    <%= check_box 'check_article', i, 
                    {:id => "article-check", :onclick => "clickMainCheckBox()"}, table_data[:article].id, false %>
                    <% else %>
                    <%= check_box 'check_article', i,
                    {:id => "article-check", :onclick => "clickCheckBoxForClip('#{@user.name}')"}, table_data[:article].id, false %>
                    <% end %>
                  </div>
                  <div class="title">
                    <a href="<%= table_data[:article].url %>" target="_blank">
                      <%= table_data[:article].title %>
                      <% if @is_login_user != true && table_data[:is_registered] %>
                      <span>
                        <% if table_data[:is_already_read] %>
                        << you already read
                        <% else %>
                        << you also register
                        <% end %>
                      </span>
                      <% end %>
                      <p>
                        <%= table_data[:registered_num] %> registered, created at <%= table_data[:registered_date].strftime("%Y/%m/%d") %> | <%= table_data[:summary_num] %>
                        <% unless table_data[:summary_num] > 1 %>
                        summary
                        <% else %>
                        summaries
                        <% end %>
                      <br>
                        <%= table_data[:article].contents_preview %> ...
                      <p>
                    </p>
                    </a>
                  </div>
                </div>
              </th>
              <th class="num-column">
                <%= link_to (url_for :controller => 'summary_lists', :action => 'index', :articleId => table_data[:article].id) do %>
                <p><%= table_data[:summary_num] %></p>
                <% unless table_data[:summary_num] > 1 %>
                <span>
                  <p>summary</p>
                </span>
                <% else %>
                <span>
                  <p>summaries</p>
                </span>
                <% end %>
                <% end %>
              </th>
            </tr>
            <% end %>
          </form>
          <% end %>
        </table>
