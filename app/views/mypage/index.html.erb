        <% if @current_tab == "favorite" %>
        <table class="favorite-table active favorite">
        <% else %>
        <table class="favorite-table favorite">
        <% end %>
          <tr class="table-top">
            <th id="article-title-top">
              <div class="article-checkbox">
                <%= check_box 'check_all', 0, 
                {:id => "favorite-check-all", :onclick => "checkAll('favorite-check-all', 'favorite-check-form')"}, 0, false %>
              </div>
              TITLE</th>
            <th id="summary-number-top">summary</th>
          </tr>
          <% if @favorite_articles_table %>
          <form id="favorite-check-form" name="favorite_checkbox">
            <% @favorite_articles_table.each_with_index do |table_data, i| %>
            <tr>
              <th>
                <div>
                  <div class="article-checkbox">
                    <% if @is_login_user %>
                    <%= check_box 'check_article', i,
                    {:id => "favorite-checkbox", :onclick => "clickFavoriteCheckBox()"}, table_data[:article].id, false %>
                    <% else %>
                    <%= check_box 'check_article', i,
                    {:id => "article-check", :onclick => "clickCheckBoxForClip('#{@user.name}')"}, table_data[:article].id, false %>
                    <% end %>
                  </div>
                  <div class="title">
                    <a href="<%= table_data[:article].url %>" target="_blank">
                      <%= table_data[:article].title %>
